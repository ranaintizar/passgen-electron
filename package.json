{
  "name": "pass-gen",
  "version": "1.1.1",
  "description": "Password Generator",
  "main": "main.js",
  "repository": "https://github.com/ranathedev/passgen-electron.git",
  "author": "ranathedev <intizaralirana2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-packager . PassGen --platform=win32 --arch=x64 --out=dist",
    "build:linux": "electron-packager . PassGen --platform=linux --arch=x64",
    "build:mac": "electron-packager . PassGen --platform=darwin --out=dist",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --linux snap"
  },
  "keywords": [
    "Password",
    "Password Generator",
    "Strong Password Generator",
    "Easy Password Generator",
    "Simple Password Generator"
  ],
  "devDependencies": {
    "electron": "^31.0.2",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "electron-store": "^10.0.0"
  }
}
